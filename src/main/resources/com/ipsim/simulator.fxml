<?import javafx.scene.layout.HBox?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.VBox?>
<HBox xmlns:fx="http://javafx.com/fxml"
  fx:controller="com.ipsim.SimulatorController" styleClass="simulator_container">
    <Button text="Registers" onAction="#showRegisters" 
            styleClass="simulator_button"
            prefHeight="200"
            prefWidth="100"/>
    <Button text="Memories" onAction="#showMemories" 
            styleClass="simulator_button"
            prefHeight="200"
            prefWidth="100"/>
    <Button text="Console"
            styleClass="simulator_button"
            prefHeight="200"
            prefWidth="100"/>
</HBox>